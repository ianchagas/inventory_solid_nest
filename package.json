{
  "name": "Melanzane",
  "version": "1.0.0",
  "description": "",
  "author": "Ian Chagas Salgado",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "16.x",
    "npm": "8.x",
    "yarn": "1.x"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --tsc",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --tsc",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "typeorm:run": "npm run build && npm run typeorm migration:run",
    "typeorm:full:run": "npm run typeorm schema:drop && npm run typeorm:run",
    "lint": "eslint \"{src,apps,libs,test}/**/**/**/*.ts\" --fix",
    "test": "jest --clearCache && jest --watch --detectOpenHandles --runInBand",
    "test:ci": "jest --clearCache && jest  --detectOpenHandles --runInBand 'a'",
    "test:watch": "jest --clearCache && jest --watchAll --runInBand --config ./jest.config.ts",
    "test:cov": "jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --watch --runInBand --config ./jest.config.json",
    "test:e2e:watch": "jest --clearCache && jest --watch --runInBand --config ./jest.config.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/testing": "^8.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "@types/express": "^4.17.13",
    "@types/supertest": "^2.0.11",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "jsonwebtoken": "^8.5.1",
    "nestjs-typeorm-paginate": "^3.1.3",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^27.0.3",
    "typeorm": "^0.2.37",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "27.0.6",
    "jest-mock-extended": "^2.0.6",
    "jest-spy-object": "^1.4.0",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@shared/shared(|/.*)$": "<rootDir>/libs/shared/src/$1",
      "^@user/user(|/.*)$": "<rootDir>/libs/user/src/$1",
      "^@people/people(|/.*)$": "<rootDir>/libs/people/src/$1",
      "^@address/address(|/.*)$": "<rootDir>/libs/address/src/$1",
      "^@unit_of_measurement/unit-of-measurement(|/.*)$": "<rootDir>/libs/unit-of-measurement/src/$1",
      "^@category/category(|/.*)$": "<rootDir>/libs/category/src/$1",
      "^@deposit/deposit(|/.*)$": "<rootDir>/libs/deposit/src/$1",
      "^@product/product(|/.*)$": "<rootDir>/libs/product/src/$1",
      "^@inventory/inventory(|/.*)$": "<rootDir>/libs/inventory/src/$1"
    }
  }
}
